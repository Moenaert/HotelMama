<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.SupervisorEntity">
        <table name="supervisor" schema="sys" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="45"/>
            </basic>
            <basic name="test">
                <column name="test" nullable="false" length="45"/>
            </basic>
            <one-to-many name="usersById" mapped-by="supervisorBySupervisorId" target-entity="entity.UsersEntity"/>
        </attributes>
    </entity>
</entity-mappings>
